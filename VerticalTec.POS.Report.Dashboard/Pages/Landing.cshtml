@page
@model VerticalTec.POS.Report.Dashboard.Pages.LandingModel
<script>
    try {
        let staff = JSON.parse(localStorage.getItem('staff'));
        if (staff) {
            location.href = '@Url.Content("~/HourlyReport")' + '?staffId=' + staff.StaffID;
        } else {
            location.href = '@Url.Content("~/Login")';
        }
    }
    catch (e) {}
</script>
